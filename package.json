    "gulp-csso": "^3.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-posthtml": "^3.0.5",
    "gulp-remember": "^1.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-webp": "^4.0.1",
    "html-validate": "5.5.0",
    "posthtml-include": "^1.7.2",
    "stylelint": "13.13.1",
    "stylelint-config-htmlacademy": "0.1.x",
    "stylelint-scss": "3.21.0",
    "webpack-stream": "6.1.2"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/scss/**/*.scss\" --fix --syntax scss",
